[% var comicBookCollection = ComicBookCollection.all.first; %]
<h2>Writers</h2>
<ul>


[% for (writer in comicBookCollection.writers) { %]
	<li>[%=writer.name%]</li>
[% } %]
</ul>

[%
var options = comicBookCollection.writers;
%]

[%for (option in options){%]
<a href="[%=option.name%].html">[%=option.name%]</a><br>
[%}%]
